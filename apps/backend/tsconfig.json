{
 "compilerOptions": {
   "target": "es5",
   "module": "commonjs",
   "strict": true,
   "esModuleInterop": true,
   "skipLibCheck": true,
   "forceConsistentCasingInFileNames": true,
   "outDir": "./dist",
   "typeRoots": [
     "node_modules/@types"
   ],
   "types": [
     "node",
     "express"
   ]
 },
 "include": [
   "src/**/*.ts"
 ],
 "exclude": [
   "node_modules"
 ]
}
